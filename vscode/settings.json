{
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "cSpell.userWords": ["mrskiro"],
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.extension.enableComposeLanguageServer": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Monaspace Argon",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "github.copilot.nextEditSuggestions.enabled": true,
  "githubPullRequests.pullBranch": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.editor.pasteUrlAsFormattedLink.enabled": "always",
  "prettier.singleQuote": false,
  "prettier.trailingComma": "es5",
  "tailwindCSS.validate": false,
  "terminal.integrated.bellDuration": 2000,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.lineHeight": 1.2,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.iconTheme": "catppuccin-latte",
  "workbench.panel.defaultLocation": "left",
  "workbench.sideBar.location": "right",
  "chat.agentsControl.enabled": false,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "json.schemaDownload.trustedDomains": {
    "https://schemastore.azurewebsites.net/": true,
    "https://raw.githubusercontent.com/": true,
    "https://www.schemastore.org/": true,
    "https://json.schemastore.org/": true,
    "https://json-schema.org/": true,
    "https://biomejs.dev": true
  },
  "git.blame.editorDecoration.enabled": true
}
